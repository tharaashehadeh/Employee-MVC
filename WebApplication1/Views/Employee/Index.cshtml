@model List<Employee>
<h1>List of Employee</h1>


<table  class="table table-success table-striped">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Age</th>
                <th scope="col">Title</th>

            </tr>
        </thead>
        <tbody>
           
           @foreach(var item in Model){
            <tr>
               <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Title</td>
            </tr>
           }
        </tbody>
    </table>
